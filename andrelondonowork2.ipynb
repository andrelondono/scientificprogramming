    "def lowest_unique_K(k, List):\n",
    "        \n",
    "    List2 = sorted(List)\n",
    "    unique = []\n",
    "    x=0\n",
    "    while(len(unique) < k ):   #if list has fewer than 10 elements then while loop will be infinite\n",
    "        if (List2[x] != List2[x+1]):\n",
    "            unique.append(List2[x])\n",
    "        x=x+1\n",
    "    return unique\n",
