    "import numpy as np\n",
    "import math\n",
    "import matplotlib.pyplot as plt\n",
    "#from __future__ import division\n",
    "def factorial(n):\n",
    "    x = 1\n",
    "    while n >= 1:\n",
    "        x = x* n\n",
    "        n = n - 1\n",
    "    return x\n",
    "\n",
    "def binomial_coefficient(n,k):\n",
    "    return float(factorial(n)/(factorial(k)*(factorial(n-k))))\n",
    "\n",
    "\n",
    "def pascals_triangle(n):\n",
    "    r = 0\n",
    "    k=0\n",
